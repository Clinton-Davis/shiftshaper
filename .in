dj() {
	python3 manage.py "$@"
}

djr() {
	python3 manage.py runserver
}

djs() {
	python3 manage.py shell
}

djts() {
	python3 manage.py tailwind start
}

env() {
	source env/bin/activate
}

makemigrations() {
	python manage.py makemigrations "$@"
}

migrate() {
	python3 manage.py migrate
}

freeze(){
	pip freeze > requirements.txt
}